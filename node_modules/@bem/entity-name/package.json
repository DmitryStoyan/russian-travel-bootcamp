  },
  "devDependencies": {
    "@types/node": "^4.2.5",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^2.1.2",
    "ava": "^0.19.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.0.0",
    "eslint-config-pedant": "^0.8.0",
    "nyc": "^10.0.0",
    "proxyquire": "^1.7.7",
    "sinon": "^2.1.0",
    "tslint": "^5.0.0",
    "tslint-config-typings": "^0.3.1",
    "typescript": "^2.2.2"
  },
  "scripts": {
