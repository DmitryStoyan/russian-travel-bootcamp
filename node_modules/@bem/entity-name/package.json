{
  "name": "@bem/entity-name",
  "version": "1.5.0",
  "description": "BEM entity name representation",
  "license": "MPL-2.0",
  "repository": "bem-sdk/bem-entity-name",
  "author": "Andrew Abramov <mail@blond.im> (github.com/blond)",
  "keywords": [
    "bem",
